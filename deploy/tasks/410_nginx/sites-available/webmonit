server {
	listen 80;
	listen [::]:80;
	server_name webmonit.ru xn--90abffb3cgipic.xn--p1ai;
	root /var/www/awt/lp-mon;
	index index.html index.php;
	location / {
		try_files $uri $uri/ =404;
	}
	location ~ \.php$ {
		expires off;
		include snippets/fastcgi-php.conf;
		fastcgi_pass unix:/var/run/php5-fpm.sock;
	}
}
